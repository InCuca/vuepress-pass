{
  "git": {
    "tagName": "v%s",
    "tagAnnotation": "Release v%s",
    "commitMessage": "Release v%s"
  },
  "github": {
    "release": true,
    "releaseName": "Release v%s"
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
